<form class="form-horizontal">
    <div class="control-group">
        <label class="control-label">{{ _('Selected UM3') }}</label>
        <div class="controls">
            <input type="text" readonly class="input-block-level" data-bind="value: currentPrinter">
            <div class="well">
                Available printers
                <ul class="unstyled" data-bind="foreach: availablePrinters">
                    <li class="btn" data-bind="text: nameWithService, click: $parent.setPrinter">
                    </li>
                </ul>
            </div>
        </div>
    </div>
</form>
